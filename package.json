{
  "name": "nuts-demo-ehr",
  "version": "1.0.0",
  "description": "A demo electronic health record system that interfaces with the Nuts node",
  "main": "index.js",
  "scripts": {
    "start": "cat NOTICE && concurrently \"npm run start_1\" \"npm run start_2\" \"npm run start_3\" \"webpack\"",
    "test": "standard && mocha --recursive --watch",
    "start_1": "CONFIG=config/zkh-noot-aan-de-man.json nodemon --inspect=0.0.0.0:9229 index.js",
    "start_2": "CONFIG=config/huisarts-nootenboom.json nodemon --inspect=0.0.0.0:9230 index.js",
    "start_3": "CONFIG=config/verpleeghuis-de-nootjes.json nodemon --inspect=0.0.0.0:9231 index.js"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "form-data": "^3.0.0",
    "nats": "^1.4.0",
    "openapi-client-axios": "^3.2.4",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "faker": "^4.1.0",
    "nodemon": "^1.19.3",
    "@nuts-foundation/irma-core": "1.0.0",
    "@nuts-foundation/irma-server": "1.0.0",
    "@nuts-foundation/irma-web": "1.0.0",
    "socket.io-client": "^2.3.0",
    "thimbleful": "^1.0.0",
